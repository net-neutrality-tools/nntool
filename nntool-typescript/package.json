{
  "name": "nntool-typescript",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run postinstall.web && ng serve web-nntool",
    "build": "npm run postinstall.web && ng build web-nntool",
    "test": "npm run postinstall.web && ng test",
    "lint": "nx lint && ng lint",
    "e2e": "npm run postinstall.web && ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "clean.shared": "cd libs/ && git clean -dfX && cd ../xplat/ && git clean -dfX",
    "clean": "npx rimraf hooks node_modules package-lock.json && npm i",
    "start.web.nntool": "npm run postinstall.web && ng serve web-nntool",
    "start.web.portal": "ng serve web-portal",
    "postinstall": "electron-rebuild install-app-deps",
    "postinstall.electron": "node tools/electron/postinstall && npm run build:ias:if-not-exists",
    "postinstall.web": "node tools/web/postinstall && npm run build:ias:if-not-exists",
    "build.electron.nntool": "npm run prepare.electron.nntool && ng build electron-nntool --prod --base-href ",
    "build.electron.nntool.local": "npm run build.electron.nntool && electron dist/apps/electron-nntool",
    "build.electron.nntool.linux": "npm run build.electron.nntool && cd dist/apps/electron-nntool && npx electron-builder build --linux --publish never",
    "build.electron.nntool.windows": "npm run build.electron.nntool && cd dist/apps/electron-nntool && npx electron-builder build --windows --publish never",
    "build.electron.nntool.mac": "npm run build.electron.nntool && cd dist/apps/electron-nntool && npx electron-builder build --mac --publish never",
    "prepare.electron.nntool": "npm run postinstall.electron && tsc -p apps/electron-nntool/tsconfig.json",
    "serve.electron.nntool.target": "ng serve electron-nntool",
    "serve.electron.nntool": "wait-on http-get://localhost:4200/ && electron apps/electron-nntool/src --serve",
    "start.electron.nntool": "npm run prepare.electron.nntool && npm-run-all -p serve.electron.nntool.target serve.electron.nntool",
    "build:ias:if-not-exists": "[ -f ../ias/build/uglified/web/ias.web.js ] && echo \"Using cached ias\" || npm run build:ias",
    "build:ias": "cd ../ias/ && npm i && ./build.sh",
    "pretty": "prettier --write \"./**/*.{ts,js,html,json}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "@ngrx/effects": "^8.2.0",
    "@ngrx/router-store": "^8.2.0",
    "@ngrx/store": "^8.2.0",
    "@ngx-translate/core": "~11.0.1",
    "@ngx-translate/http-loader": "~4.0.0",
    "@nntool-typescript/less": "file:libs/less",
    "@nntool-typescript/web": "file:xplat/web",
    "@nrwl/nx": "^7.8.7",
    "ng2-completer": "^3.0.2",
    "ng2-nvd3": "~2.0.0",
    "ng2-smart-table": "^1.5.0",
    "ngx-device-detector": "~1.3.9",
    "ngx-logger": "^4.0.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.2",
    "zone.js": "~0.10.1",
    "leaflet": "~1.5.1",
    "@asymmetrik/ngx-leaflet": "~6.0.1",
    "leaflet-bing-layer": "~3.3.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~8.1.0",
    "@angular-devkit/build-angular": "~0.802.0",
    "@angular/cli": "~8.2.0",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.0",
    "@nrwl/schematics": "^8.4.4",
    "@nrwl/workspace": "8.4.4",
    "@nstudio/schematics": "^8.0.5",
    "@types/jasmine": "~3.3.16",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~12.7.0",
    "@types/leaflet": "~1.5.2",
    "asciidoctor.js": "~1.5.9",
    "codelyzer": "~5.1.0",
    "dotenv": "8.0.0",
    "electron": "5.0.12",
    "electron-builder": "^20.28.4",
    "electron-installer-dmg": "~3.0.0",
    "electron-packager": "~14.0.4",
    "electron-rebuild": "~1.8.6",
    "electron-reload": "~1.5.0",
    "electron-store": "~4.0.0",
    "electron-updater": "~4.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~3.0.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "protractor": "~5.4.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.5.3",
    "uglify-es": "~3.3.9",
    "wait-on": "~3.3.0"
  },
  "xplat": {
    "prefix": "nntool"
  }
}

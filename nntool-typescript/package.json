{
  "name": "nntool-typescript",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run postinstall.web && ng serve web-nntool",
    "build": "npm run postinstall.web && ng build web-nntool",
    "test": "npm run postinstall.web && ng test",
    "lint": "nx lint && ng lint",
    "e2e": "npm run postinstall.web && ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "clean.shared": "cd libs/ && git clean -dfX && cd ../xplat/ && git clean -dfX",
    "clean": "npx rimraf hooks node_modules package-lock.json && npm i",
    "start.web.nntool": "npm run postinstall.web && ng serve web-nntool",
    "start.web.portal": "ng serve web-portal",
    "postinstall": "electron-rebuild install-app-deps",
    "postinstall.electron": "node tools/electron/postinstall && npm run build:ias",
    "postinstall.web": "node tools/web/postinstall && npm run build:ias",
    "build.electron.nntool": "npm run prepare.electron.nntool && ng build electron-nntool --prod --base-href ./",
    "build.electron.nntool.local": "npm run build.electron.nntool && electron dist/apps/electron-nntool",
    "build.electron.nntool.linux": "npm run build.electron.nntool && cd dist/apps/electron-nntool && npx electron-builder build --linux",
    "build.electron.nntool.windows": "npm run build.electron.nntool && cd dist/apps/electron-nntool && npx electron-builder build --windows",
    "build.electron.nntool.mac": "npm run build.electron.nntool && cd dist/apps/electron-nntool && npx electron-builder build --mac",
    "prepare.electron.nntool": "npm run postinstall.electron && tsc -p apps/electron-nntool/tsconfig.json",
    "serve.electron.nntool.target": "ng serve electron-nntool",
    "serve.electron.nntool": "wait-on http-get://localhost:4200/ && electron apps/electron-nntool/src --serve",
    "start.electron.nntool": "npm run prepare.electron.nntool && npm-run-all -p serve.electron.nntool.target serve.electron.nntool",
    "build:ias": "cd ../ias/ && ./build.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "rxjs": "~6.4.0",
    "zone.js": "~0.9.1",
    "@ngx-translate/core": "~11.0.1",
    "@ngx-translate/http-loader": "~4.0.0",
    "@nrwl/nx": "^7.8.0",
    "@ngrx/effects": "^8.0.1",
    "@ngrx/router-store": "^8.0.1",
    "@ngrx/store": "^8.0.1",
    "@nntool-typescript/scss": "file:libs/scss",
    "@nntool-typescript/web": "file:xplat/web",
    "reflect-metadata": "^0.1.12",
    "ng2-nvd3": "~2.0.0",
    "ngx-device-detector": "~1.3.5"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~8.0.2",
    "@angular-devkit/build-angular": "~0.800.1",
    "@angular/cli": "~8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@nrwl/schematics": "^8.1.2",
    "@nrwl/workspace": "8.1.2",
    "@nstudio/schematics": "^7.4.8",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "asciidoctor.js": "~1.5.9",
    "codelyzer": "~5.1.0",
    "dotenv": "6.2.0",
    "electron": "^5.0.5",
    "electron-builder": "^20.44.4",
    "electron-installer-dmg": "~3.0.0",
    "electron-packager": "~14.0.0",
    "electron-rebuild": "~1.8.5",
    "electron-reload": "~1.4.0",
    "electron-store": "~4.0.0",
    "electron-updater": "~4.0.6",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "1.16.4",
    "protractor": "~5.4.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.3",
    "wait-on": "~3.2.0",
    "uglify-es": "~3.3.9"
  },
  "xplat": {
    "prefix": "nntool"
  }
}

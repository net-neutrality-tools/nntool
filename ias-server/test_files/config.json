{
  "ip_bindings": {
    "v4": "",
    "v6": ""
  },
  "port_bindings": {
    "tcp": 80,
    "tcp_tls": 443,
    "tcp_traceroute": 8080,
    "tcp_traceroute_tls": 8443,
    "udp": 80
  },
  "authentication": {
    "enabled" : false,
    "secret": "my_auth_secret",
    "max_age": 120
  },
  "hostname": "my_hostname",
  "tcp_timeout": "25",
  "random_data_size": 1123457,
  "load": {
    "monitoring": {
      "enabled": true,
      "interface": "lo",
      "bandwidth": {
        "bits_max": 1000000000,
        "bits_free": 200000000
      }
    },
    "balancer": {
      "enabled": true,
      "port": 44301,
      "secret": "none"
    }
  },
  "collector": {
    "enabled": false,
    "url": "https://my_collector_url.eu",
    "secret": "my_collector_secret"
  }
}